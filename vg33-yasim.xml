<!-- Arsenal VG33 - 2007 BARANGER Emmanuel -->

<airplane mass="2850" >

  <approach speed="80" aoa="5" >
    <control-setting axis="/controls/engines/engine[0]/throttle" value="0.2"/>
    <control-setting axis="/controls/engines/engine[0]/mixture" value="1.0"/>
    <solve-weight idx="0" weight="180"/>
    <solve-weight idx="1" weight="0"/>
  </approach>
  <cruise speed="400" alt="5000" >
    <control-setting value="1.0" axis="/controls/engines/engine[0]/throttle" />
    <control-setting value="1.0" axis="/controls/engines/engine[0]/mixture" />
    <solve-weight idx="0" weight="180"/>
    <solve-weight idx="1" weight="0"/>
  </cruise>

  <cockpit x="-0.034" y="0" z="0.635"/>

  <fuselage ax="4.198" ay="0.0" az="-0.043" bx="-4.198" by="0" bz="0.111" width="0.965" taper="0.3" midpoint="0.2"/>

  <wing x="1.141" y="0.428" z="-0.374" 
        taper="0.5" 
        incidence="0.06"
	length="4.661" 
	chord="2.00" 
	sweep="0.1" 
	dihedral="6.0" 
	camber="0.1" 
	twist="0.2" 
	idrag="1.25">
    <stall aoa="14" width="5" peak="1.5"/>
    <flap0 start="0.0" end="0.4" lift="1.7" drag="1.9"/>
    <flap1 start="0.4" end="1.0" lift="1.4" drag="1.1"/>
    <control-input axis="/controls/flight/flaps" control="FLAP0"/>
    <control-output control="FLAP0" prop="/surface-positions/flap-pos-norm"/>
    <control-speed control="FLAP0" transition-time="7"/>
    <control-input axis="/controls/flight/aileron" control="FLAP1" split="true"/>
    <control-output control="FLAP1" side="left" prop="surface-positions/left-aileron-pos-norm"/>
    <control-output control="FLAP1" side="right" prop="surface-positions/right-aileron-pos-norm"/>
    <control-speed control="FLAP1" transition-time="1"/>
    <control-input axis="/controls/flight/aileron-trim" control="FLAP1" split="true"/>
  </wing>
  
  <hstab x="-2.992" y="0.217" z="0.246" 
         taper="0.55" 
	 length="1.643" 
	 chord="1.155" 
	 sweep="0" 
	 incidence="1.0"> 
    <stall aoa="17" width="8" peak="1.5"/>
    <flap0 start="0.0" end="1.6" lift="1.55" drag="1.3"/>
    <control-input axis="/controls/flight/elevator" control="FLAP0"/>
    <control-input axis="/controls/flight/elevator-trim" control="FLAP0"/>
    <control-output control="FLAP0" prop="/surface-positions/elevator-pos-norm"/>
  </hstab>

  <vstab x="-3.299" y="0" z="0.383" 
         taper="0.38" 
	 length="1.154" 
	 chord="1.624" 
	 sweep="15" 
	 incidence="-0.30">
    <stall aoa="15" width="4" peak="1.5"/>
    <flap0 start="0" end="1" lift="1.3" drag="1.3"/>
    <control-input axis="/controls/flight/rudder" square="true" control="FLAP0" invert="true"/>
    <control-input axis="/controls/flight/rudder-trim" control="FLAP0" invert="true"/>
    <control-output control="FLAP0" prop="/surface-positions/rudder-pos-norm"
		    min="1" max="-1"/>
  </vstab>

  <!-- Lorraine 12 R.C.R "Radium" (2200 ch)  
       V12 60° inversés suralimenté
       2200 ch à 3900 tr/m 
       Poids : 510 kg -> 1124.3 lb          -->
  <propeller moment="35" 
             x="2.458" 
             y="0.0" 
             z="0.231" 
             mass="1124" 
             radius="1.2"
             cruise-power="2000" 
             cruise-alt="5000" 
             min-rpm="1400" 
             max-rpm="3900" 
             cruise-speed="400" 
             cruise-rpm="2200" 
             fine-stop="0.9" 
             coarse-stop="2.5" 
             contra="1">
             <piston-engine eng-rpm="3900" 
                            alt="0" 
                            eng-power="2200"
                            displacement="985" 
                            compression="6"/>
             <actionpt x="3.613" y="0.0" z="0.383" />
             <control-input control="THROTTLE" axis="/controls/engines/engine[0]/throttle"/>
             <control-input control="STARTER" axis="/controls/engines/engine[0]/starter"/>
             <control-input control="MAGNETOS" axis="/controls/engines/engine[0]/magnetos"/>
             <control-input control="MIXTURE" axis="/controls/engines/engine[0]/mixture"/>
             <control-input control="ADVANCE" axis="/controls/engines/engine[0]/propeller-pitch"/>
  </propeller>

  <!-- left main -->
  <gear x="1.484" 
        y="1.944" 
        z="-1.629" 
        compression=".4" retract-time="5" spring="0.8" damp="1.2" sfric="1.0" dfric="1.0">
        <control-input axis="/controls/gear/brake-left" control="BRAKE"/>
        <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
        <control-input axis="/controls/gear/gear-down" control="EXTEND"/>

        <control-speed control="EXTEND" transition-time="5.1"/>

        <control-output control="EXTEND" prop="/gear/gear[0]/position-norm"/>
  </gear>

  <!-- right main -->
  <gear x="1.484" 
        y="-1.944" 
        z="-1.629" 
        compression=".4" retract-time="5" spring="0.8" damp="1.2" sfric="1.0" dfric="1.0">
        <control-input axis="/controls/gear/brake-right" control="BRAKE"/>
        <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
        <control-input axis="/controls/gear/gear-down" control="EXTEND"/>

        <control-speed control="EXTEND" transition-time="5.1"/>

        <control-output control="EXTEND" prop="/gear/gear[1]/position-norm"/>
  </gear>

  <!-- Tail wheel; has castering selectable by a wheel lock -->
  <gear x="-3.682"
        y="0"
        z="-0.311"
        compression="0.2" pring="0.8" damp="1.2" sfric="1.0" dfric="1.0">
  </gear>

  <!-- Fuel  -->  
  <tank x="1.5" y="0" z="-0.7" capacity="210" />
  <tank x="0.0" y="0" z="-0.7" capacity="210" />
  <tank x="-1.5" y="0" z="-0.7" capacity="156" />

  <weight x="0.75" y="0.0" z="-0.5" mass-prop="/sim/weight[0]/weight-lb"/>
  <weight x="-1.0" y="0" z="-0.5" mass-prop="/sim/weight[1]/weight-lb"/>

  <!-- CG théorique centre de la corde x=1.141 y=0.0 z=-0.374 -->
  <ballast x="1.8" y="0.0" z="-3.67" mass="350" />

</airplane>
